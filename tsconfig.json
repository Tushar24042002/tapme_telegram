{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs", // Ensure this is set to 'commonjs'
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist"
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
